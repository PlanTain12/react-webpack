{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "react-webpack template",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "NODE_ENV=development webpack serve --config webpack.config.js",
